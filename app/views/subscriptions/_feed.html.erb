<% @entries.each do |entry| %>
<section id="feed">
  <h3><%= entry.title %></h3>
  <p><%= entry.summary.html_safe unless entry.content.nil? %></p>
  <div class="bs-docs-example">
    <div class="tabbable" style="margin-bottom: 18px;">
      <%= entry.content.nil?? entry.summary.html_safe : entry.content.html_safe %>
    </div> <!-- /tabbable -->
  </div>
</section>
<% end %>
