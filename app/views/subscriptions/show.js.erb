<% if @feed.entries.all.count>0 -%>
$('#feed-view').append('<%= escape_javascript(render(partial: 'feed', object: @feed)) %>');
$('a.hook').attr('href', '<%= subscription_path(id: @feed.id, page: (params[:page].to_i || 1)+1, per_page: params[:per_page] || 15) %>').show();
<% else -%>
$('a.hook').hide();
<% end -%>
